{
  "name": "@ptomulik/github-list-releases-action",
  "version": "0.2.0",
  "description": "GitHub action to retrieve releases from a remote repository",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --coverage",
    "tsd": "tsd",
    "pretsd": "npm run compile",
    "compile": "tsc",
    "build": "ncc build",
    "prebuild": "npm run compile",
    "prettier": "prettier",
    "lint": "eslint '**/*.ts'",
    "lint-fix": "eslint --fix '**/*.ts'",
    "pre-checkin": "npm run lint && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptomulik/github-list-releases-action"
  },
  "keywords": [],
  "author": "Pawe≈Ç Tomulik <ptomulik@meil.pw.edu.pl>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@octokit/core": "^3.2.5",
    "@octokit/plugin-paginate-rest": "^2.9.1",
    "@octokit/plugin-rest-endpoint-methods": "^4.10.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vercel/ncc": "^0.26.2",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tsd": "^0.14.0",
    "typescript": "^4.1.3"
  },
  "tsd": {
    "compilerOptions": {
      "module": "commonjs",
      "outDir": "./lib",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "target": "es5",
      "lib": [
        "es5"
      ]
    }
  }
}
